<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context="hzu.android.labs.Net1814080903304Activity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="欢迎来到首页" />

    <Button
        android:id="@+id/button_object"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_object" />

    <Button
        android:id="@+id/button_reflect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_reflect"/>

    <Button
        android:id="@+id/button_teacher"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_teacher"/>




</LinearLayout>