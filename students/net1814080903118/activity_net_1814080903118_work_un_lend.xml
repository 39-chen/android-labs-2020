<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/work_bg"
    tools:context=".Net1814080903118WorkUnLendActivity">

    <Button
        android:id="@+id/navPersonalBtn1"
        android:layout_width="206dp"
        android:layout_height="43dp"
        android:background="@drawable/personal_logo"
        android:paddingLeft="10dp"
        android:text="个人"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/navLendBtn1" />

    <ImageView
        android:id="@+id/lendMap"
        android:layout_width="108dp"
        android:layout_height="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/map_icon" />

    <ImageView
        android:id="@+id/scanWin"
        android:layout_width="257dp"
        android:layout_height="313dp"
        android:layout_marginTop="156dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/scan_icon" />

    <Button
        android:id="@+id/navLendBtn1"
        android:layout_width="202dp"
        android:layout_height="43dp"
        android:background="@drawable/work_logo"
        android:paddingLeft="10dp"
        android:text="租借"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>